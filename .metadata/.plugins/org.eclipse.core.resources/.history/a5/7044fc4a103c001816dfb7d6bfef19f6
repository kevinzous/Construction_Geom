
public abstract class Point extends ElementsGeometriques {
	private double abscisse;
	private double ordonnee; 
	
	
	public Point(String myName,double Myabscisse,double Myordonnee) {
		super(myName);
		this.abscisse = Myabscisse;
		this.ordonnee = Myordonnee;
	}
	
	public abstract double GetCoordonnees() ;
}
